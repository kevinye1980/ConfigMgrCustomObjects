        </Field>
        <Field Name="DiskSize">
          <DataField>DiskSize</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>  SELECT distinct SYS.Netbios_Name0, SYS.User_Name0, SYS.User_Domain0,  SYS.Resource_Domain_OR_Workgr0,
            OPSYS.Caption0 as C054, OPSYS.Version0,
            MEM.TotalPhysicalMemory0,
            STUFF((SELECT (N','+IPAddr.IP_Addresses0) AS [text()]
            FROM v_RA_System_IPAddresses  IPAddr
            WHERE SYS.ResourceID = IPAddr.ResourceID for xml path(N''))
            ,1,1,N'') as IP_Addresses0, -- if there are multiple IP address then combine them together
            CSYS.Model0, Processor.NumberOfCores, HDisk.TotalDiskSize,
            STUFF((SELECT (N'+'+ cast(HDD.Size0 as varchar(20))) AS [text()]
            FROM v_GS_PARTITION HDD
            WHERE SYS.ResourceID = HDD.ResourceID for xml path(N''))
            ,1,1,N'') as DiskSize
            FROM v_R_System  SYS
            LEFT JOIN  v_GS_X86_PC_MEMORY  MEM on SYS.ResourceID = MEM.ResourceID
            LEFT JOIN  v_GS_COMPUTER_SYSTEM  CSYS on SYS.ResourceID = CSYS.ResourceID
            LEFT JOIN  (select ResourceID, Sum(NumberOfCores0) as NumberOfCores from v_GS_PROCESSOR group by ResourceID)  Processor  on SYS.ResourceID = Processor.ResourceID 
            LEFT JOIN (select  ResourceID,SUM(Size0)/1024 as TotalDiskSize from v_GS_PARTITION group by ResourceID) HDisk on SYS.ResourceID = HDisk.ResourceID
            LEFT JOIN v_GS_OPERATING_SYSTEM  OPSYS on SYS.ResourceID=OPSYS.ResourceID
            INNER JOIN v_FullCollectionMembership FCM on SYS.ResourceID = FCM.ResourceID
            WHERE FCM.CollectionID = @CollName
            ORDER BY SYS.Netbios_Name0, SYS.Resource_Domain_OR_Workgr0</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CollName">
            <Value>=Parameters!CollName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet1">
