  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SCCM2016DS</DataSourceName>
        <CommandText>select RSYS.Name0 as ComputerName,UIG.Group0 as GroupName, (UIG.Domain0 + '\' + UIG.Name0) as UserName, 
UIG.Type0 as IsLocalOrDomainAccount
 from v_GS_UserInLocalGroup  UIG
inner join v_R_System RSYS on UIG.ResourceID = RSYS.ResourceID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
